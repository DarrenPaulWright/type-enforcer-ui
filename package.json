{
	"name": "type-enforcer-ui",
	"version": "1.0.0-beta.2",
	"description": "An extension of type-enforcer with UI related data types and enforcer functions",
	"main": "index.js",
	"sideEffects": false,
	"scripts": {
		"test": "karma start --single-run",
		"test-watch": "karma start",
		"version": "npm run docs && git add docs",
		"docs:readme": "jsdoc2md index.js > README.md",
		"docs:enforce": "jsdoc2md src/enforcer/**/*.js > docs/enforce.md",
		"docs:method": "jsdoc2md src/methods/**/*.js > docs/method.md",
		"docs:checks": "jsdoc2md src/checks/**/*.js > docs/checks.md",
		"docs:csssize": "jsdoc2md src/CssSize.js > docs/CssSize.md",
		"docs:dockpoint": "jsdoc2md src/DockPoint.js > docs/DockPoint.md",
		"docs:thickness": "jsdoc2md src/Thickness.js > docs/Thickness.md",
		"docs": "run-p \"docs:* -- -t docs/docs.hbs --plugin dmd-readable\""
	},
	"badges": [
		{
			"name": "npm",
			"image": "https://img.shields.io/npm/v/type-enforcer-ui.svg",
			"url": "https://npmjs.com/package/type-enforcer-ui"
		},
		{
			"name": "build",
			"image": "https://travis-ci.org/DarrenPaulWright/type-enforcer-ui.svg?branch=master",
			"url": "https://travis-ci.org/DarrenPaulWright/type-enforcer-ui"
		},
		{
			"name": "coverage",
			"image": "https://coveralls.io/repos/github/DarrenPaulWright/type-enforcer-ui/badge.svg?branch=master",
			"url": "https://coveralls.io/github/DarrenPaulWright/type-enforcer-ui?branch=master"
		},
		{
			"name": "deps",
			"image": "https://david-dm.org/darrenpaulwright/type-enforcer-ui.svg",
			"url": "https://david-dm.org/darrenpaulwright/type-enforcer-ui"
		},
		{
			"name": "size",
			"image": "https://packagephobia.now.sh/badge?p=type-enforcer-ui",
			"url": "https://packagephobia.now.sh/result?p=type-enforcer-ui"
		},
		{
			"name": "vulnerabilities",
			"image": "https://snyk.io/test/github/DarrenPaulWright/type-enforcer-ui/badge.svg?targetFile=package.json",
			"url": "https://snyk.io/test/github/DarrenPaulWright/type-enforcer-ui?targetFile=package.json"
