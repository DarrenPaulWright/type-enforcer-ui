{
	"name": "type-enforcer-ui-addon",
	"version": "1.0.0-beta.0",
	"description": "An addon for type-enforcer with UI related data types and enforcer functions",
	"main": "index.js",
	"scripts": {
		"test": "karma start --single-run",
		"test-watch": "karma start",
		"patch": "npm version patch -m \"Increment patch version to %s\" && npm publish",
		"minor": "npm version minor -m \"Increment minor version to %s\" && npm publish",
		"major": "npm version major -m \"Increment major version to %s\" && npm publish",
		"docs-readme": "jsdoc2md index.js > README.md -t docs/docs.hbs --plugin dmd-readable",
		"docs-enforce": "jsdoc2md src/enforcer/**/*.js > docs/enforce.md -t docs/docs.hbs --plugin dmd-readable",
		"docs-method": "jsdoc2md src/methods/**/*.js > docs/method.md -t docs/docs.hbs --plugin dmd-readable",
		"docs-checks": "jsdoc2md src/checks/**/*.js > docs/checks.md -t docs/docs.hbs --plugin dmd-readable",
		"docs-csssize": "jsdoc2md src/CssSize.js > docs/CssSize.md -t docs/docs.hbs --plugin dmd-readable",
		"docs-dockpoint": "jsdoc2md src/DockPoint.js > docs/DockPoint.md -t docs/docs.hbs --plugin dmd-readable",
		"docs-thickness": "jsdoc2md src/Thickness.js > docs/Thickness.md -t docs/docs.hbs --plugin dmd-readable",
		"docs": "npm run docs-readme && npm run docs-enforce && npm run docs-method && npm run docs-checks && npm run docs-csssize && npm run docs-dockpoint && npm run docs-thickness"
	},
	"badges": [
		{
